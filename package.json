{
  "name": "ac-product-block",
  "version": "1.0.0",
  "scripts": {
    "build": "wp-scripts build",
    "start": "wp-scripts start",
    "lint:css": "stylelint '**/*.scss'",
    "lint:css:fix": "stylelint '**/*.scss' --fix",
    "lint:js": "eslint '**/*.{js,jsx}'",
    "lint:js:fix": "eslint '**/*.{js,jsx}' --fix",
    "lint:php": "php -d memory_limit=512M vendor/bin/phpcs --standard=phpcs.xml -s -v",
    "lint:php:fix": "php -d memory_limit=512M vendor/bin/phpcbf --standard=phpcs.xml"
  },
  "devDependencies": {
    "@wordpress/scripts": "^24.0.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "stylelint": "^16.9.0",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-scss": "^6.6.0"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^7.8.0",
    "@wordpress/element": "^6.8.0"
  }
}
